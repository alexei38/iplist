<div class="container">
  <div id="content">
    <div class="well">
      <% lists = List.all %>
      <a href="<%= new_list_path %>" class="btn btn-success"><i class="icon-plus icon-white"></i><span class="hidden-tablet"> Создать</span></a>
      <br />
      <table class="table table-iplist">
        <thead>
        <tr>
          <th>Фио</th><th>Отдел</th><th>Кабинет</th><th>Имя ПК</th><th>IP</th><th>MAC</th><th>OS</th><th>Паспорт</th><th>Действие</th>
        </tr>
        </thead>
        <tbody>
        <% lists.each do |list| %>
          <tr valign="center">
            <td><%= list.fio %></td>
            <td><%= list.otdel %></td>
            <td><%= list.kabinet %></td>
            <td><%= list.name_pk %></td>
            <td><%= list.ip %></td>
            <td><%= list.mac %></td>
            <td><%= list.os %></td>
            <td><%= list.passport %></td>
            <td>
              <p><%= link_to 'Edit', edit_list_path(list), :class => "btn btn-success btn-mini" %></p>
              <p><%= link_to 'Remove', list_path(list), :confirm => 'Are you sure?', :method => :delete, :class => "btn btn-danger btn-mini" %></p>
              <p><%= link_to 'Check', "#", :class => "btn btn-primary btn-mini" %></p>
            </td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
